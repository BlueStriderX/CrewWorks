package crewworks.main;

//class imports
import crewspace.structure;

public static file mod = modinfo.json;
public static this.MODNAME = mod.MODNAME;
public static this.MODVERSION = mod.MODVERSION;
public static this.MODAUTHOR = mod.MODAUTHOR;
public static this.MODDESCRIPTION = mod.MODDESCRIPTION;

public static void onInitialize() {
  System.out.println("CrewWorks is loading!");
  try {
    initialize();
  } catch(Exception e) {
    System.out.println("CrewWorks failed to load!")
    e.printStackTrace(System.out);
    System.out.println("Disabling CrewWorks...")
  }
}

public static void initialize() {
  structure.main;
}
